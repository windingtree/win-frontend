{
  "name": "stays",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "cra-webpack-rewired start",
    "build": "cra-webpack-rewired build",
    "test": "cra-webpack-rewired test",
    "storybook": "start-storybook -p 6006 -s public",
    "commit": "git-cz -S",
    "lint": "npx eslint . --ext .ts,.tsx && npx prettier -c .",
    "prettier": "npx prettier -w ./docs ./src ./README.md"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
